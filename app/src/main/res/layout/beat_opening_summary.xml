<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        style="@style/CardViewStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/padding_normal_4">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:id="@+id/constraintLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="MissingConstraints">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_4sdp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4sdp">

                        <TextView
                            android:id="@+id/tv_total_rel"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="@string/lbl_total_ret"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@+id/tv_visited_ret"
                            android:gravity="center"

                            android:layout_marginRight="4dp"
                            tools:ignore="MissingConstraints" />

<!--

                        <TextView
                            android:id="@+id/tv_target_val"
                            style="@style/SubItemTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:gravity="right"
                            android:textSize="@dimen/util_normal_text_sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/guideline4" />
-->

                        <TextView
                            android:id="@+id/tv_visited_ret"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="@string/lbl_visited"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_total_rel"
                            app:layout_constraintRight_toLeftOf="@+id/tv_non_visited_ret"

                            android:layout_marginRight="4dp"
                            tools:ignore="MissingConstraints" />
                        <TextView
                            android:id="@+id/tv_non_visited_ret"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="@string/lbl_non_visited"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_visited_ret"
                            app:layout_constraintRight_toRightOf="parent"

                            android:layout_marginRight="4dp"
                            tools:ignore="MissingConstraints" />


                      <!--  <android.support.constraint.Guideline
                            android:id="@+id/guideline4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="141dp"
                            android:orientation="vertical"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintGuide_percent="0.45"
                            app:layout_constraintStart_toEndOf="@+id/tv_achived_label"
                            android:layout_marginRight="141dp" />-->
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/tv_total_ret_val"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="4dp"
                            android:gravity="center"
                            android:text="0"
                            app:layout_constraintHorizontal_weight="1"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@+id/tv_Visited_ret_val"
                            tools:ignore="MissingConstraints" />

                        <!--

                                                <TextView
                                                    android:id="@+id/tv_target_val"
                                                    style="@style/SubItemTextStyle"
                                                    android:layout_width="0dp"
                                                    android:layout_height="wrap_content"
                                                    android:text=""
                                                    android:gravity="right"
                                                    android:textSize="@dimen/util_normal_text_sp"
                                                    app:layout_constraintEnd_toEndOf="parent"
                                                    app:layout_constraintStart_toStartOf="@+id/guideline4" />
                        -->

                        <TextView
                            android:id="@+id/tv_Visited_ret_val"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="0"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_total_ret_val"
                            app:layout_constraintRight_toLeftOf="@+id/tv_non_visited_ret_val"
                            android:layout_marginRight="4dp"
                            tools:ignore="MissingConstraints" />
                        <TextView
                            android:id="@+id/tv_non_visited_ret_val"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            android:textColor="@color/RED"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="0"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_Visited_ret_val"
                            app:layout_constraintRight_toRightOf="parent"
                            android:layout_marginRight="4dp" />


                        <!--  <android.support.constraint.Guideline
                              android:id="@+id/guideline4"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_marginEnd="141dp"
                              android:orientation="vertical"
                              app:layout_constraintEnd_toEndOf="parent"
                              app:layout_constraintGuide_percent="0.45"
                              app:layout_constraintStart_toEndOf="@+id/tv_achived_label"
                              android:layout_marginRight="141dp" />-->
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/_4sdp"
                    android:paddingRight="@dimen/_4sdp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >

                        <TextView
                            android:id="@+id/tv_prod_rel"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="@string/lbl_tprod"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@+id/tv_non_prod_ret"
                            android:gravity="center"

                            android:layout_marginRight="4dp" />

                        <!--

                                                <TextView
                                                    android:id="@+id/tv_target_val"
                                                    style="@style/SubItemTextStyle"
                                                    android:layout_width="0dp"
                                                    android:layout_height="wrap_content"
                                                    android:text=""
                                                    android:gravity="right"
                                                    android:textSize="@dimen/util_normal_text_sp"
                                                    app:layout_constraintEnd_toEndOf="parent"
                                                    app:layout_constraintStart_toStartOf="@+id/guideline4" />
                        -->

                        <TextView
                            android:id="@+id/tv_non_prod_ret"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="@string/lbl_non_prod"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_prod_rel"
                            app:layout_constraintRight_toLeftOf="@+id/tv_no_order_ret"

                            android:layout_marginRight="4dp" />
                        <TextView
                            android:id="@+id/tv_no_order_ret"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="@string/lbl_no_order"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_non_prod_ret"
                            app:layout_constraintRight_toRightOf="parent"

                            android:layout_marginRight="4dp" />


                        <!--  <android.support.constraint.Guideline
                              android:id="@+id/guideline4"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_marginEnd="141dp"
                              android:orientation="vertical"
                              app:layout_constraintEnd_toEndOf="parent"
                              app:layout_constraintGuide_percent="0.45"
                              app:layout_constraintStart_toEndOf="@+id/tv_achived_label"
                              android:layout_marginRight="141dp" />-->
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_8sdp"
                        android:layout_height="wrap_content">

                        <!--

                                                <TextView
                                                    android:id="@+id/tv_target_val"
                                                    style="@style/SubItemTextStyle"
                                                    android:layout_width="0dp"
                                                    android:layout_height="wrap_content"
                                                    android:text=""
                                                    android:gravity="right"
                                                    android:textSize="@dimen/util_normal_text_sp"
                                                    app:layout_constraintEnd_toEndOf="parent"
                                                    app:layout_constraintStart_toStartOf="@+id/guideline4" />
                        -->

                        <TextView
                            android:id="@+id/tv_prod_ret_val"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="4dp"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="@color/green_light"
                            app:layout_constraintHorizontal_weight="1"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@+id/tv_non_prod_ret_val"
                            tools:layout_editor_absoluteY="0dp" />

                        <TextView
                            android:id="@+id/tv_non_prod_ret_val"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="0"
                            android:gravity="center"
                            android:textColor="@color/colorYellow"
                            app:layout_constraintLeft_toRightOf="@+id/tv_prod_ret_val"
                            app:layout_constraintRight_toLeftOf="@+id/tv_no_order_ret_val"
                            android:layout_marginRight="4dp" />
                        <TextView
                            android:id="@+id/tv_no_order_ret_val"
                            style="@style/UtilsMediumTextStyle"
                            android:layout_width="0dp"
                            android:textColor="@color/InvStatusOrange"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            app:layout_constraintHorizontal_weight="1"
                            android:text="0"
                            android:gravity="center"
                            app:layout_constraintLeft_toRightOf="@+id/tv_non_prod_ret_val"
                            app:layout_constraintRight_toRightOf="parent"
                            android:layout_marginRight="4dp" />


                        <!--  <android.support.constraint.Guideline
                              android:id="@+id/guideline4"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_marginEnd="141dp"
                              android:orientation="vertical"
                              app:layout_constraintEnd_toEndOf="parent"
                              app:layout_constraintGuide_percent="0.45"
                              app:layout_constraintStart_toEndOf="@+id/tv_achived_label"
                              android:layout_marginRight="141dp" />-->
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</LinearLayout>